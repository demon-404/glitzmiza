{
  "version": 2,
  "builds": [
    {
      "src": "FRONT-END/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "FRONT-END/dist"
      }
    },
    {
      "src": "BACKEND/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "BACKEND/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "FRONT-END/dist/$1"
    }
  ]
}