{
  "version": 2,
  "builds": [
    {
      "src": "FRONT-END/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "BACKEND/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "BACKEND/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "FRONT-END/$1"
    }
  ]
}